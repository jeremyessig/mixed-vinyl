{% extends "base.html.twig" %}

{% block title %}New | {{ parent() }} {% endblock %}

{% block body %}
    
<h1>{{ title }}</h1>

<div>

    <h4>Tracks</h4>

    <ul>
    {% for track in tracks %}
        <li>
            {{ track.song }} - {{ track.artist }}
        </li>
    {% endfor %}

    </ul>

</div>
{% endblock %}